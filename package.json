{
  "name": "analytics-ingest-lambda",
  "version": "0.0.0",
  "engines": {
    "node": "6.10.3"
  },
  "description": "Process kinesis metric streams and ship to bigquery",
  "main": "index.js",
  "scripts": {
    "geolite": "node bin/getmaxmind.js",
    "start": "node-lambda run -j test/support/test-event.json",
    "test": "mocha test/",
    "ci": "istanbul cover _mocha -- test/ && codecov",
    "watch": "mocha test/ -w",
    "deploy-dev": "node bin/deploy development",
    "deploy-staging": "node bin/deploy staging",
    "deploy-prod": "node bin/deploy production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PRX/analytics-ingest-lambda.git"
  },
  "keywords": [
    "lambda",
    "kinesis",
    "bigquery"
  ],
  "author": "ryan cavis <ryan@prx.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PRX/analytics-ingest-lambda/issues"
  },
  "homepage": "https://github.com/PRX/analytics-ingest-lambda#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "dotenv": "^4.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nock": "^9.0.14",
    "node-lambda": "0.8.13",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.9.0",
    "targz": "^1.0.1",
    "uuid": "^3.1.0"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^0.7.0",
    "aws-sdk": "^2.19.0",
    "maxmind": "^2.2.0",
    "prx-podagent": "0.0.2",
    "urijs": "^1.19.0"
  }
}
